{
  "name": "nebula",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check": "turbo check",
    "lint": "turbo lint",
    "test": "turbo test",
    "prepare": "husky"
  },
  "lint-staged": {
